    "prettier": "^3.0.3",
    "sass": "^1.70.0",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.5",
    "vitest": "^1.0.1",
    "vue-cli-plugin-tailwind": "~3.0.0"
